{
    "compilerOptions": {
        "target": "es5",
        "skipLibCheck": true,
        "strict": true,
        "lib": ["es5", "dom"],
        "types": ["cypress", "node"],
        "resolveJsonModule": true,
        "esModuleInterop": true
    },
    "include": ["cypress/**/*.ts", "cypress.config.ts"],
    "exclude": ["node_modules/"]
}